{   "description": "Descarga videos y subtitulos de cuevana2.tv, facebook, vimeo y cualquier sitio similar a estos",
    "homepage_url": "http://danielarriola.com/",
    "icons": {
        "128": "icon128.png",
        "16": "icon16.png",
        "48": "icon48.png"
    },
  "browser_action": {
    "default_icon": {
      "19": "icon16.png"
    },
